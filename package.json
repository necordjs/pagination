{
	"name": "@necord/pagination",
	"description": "A lightweight Pagination module for Necord",
	"version": "2.2.0",
	"scripts": {
		"build": "rimraf -rf dist && tsc -p tsconfig.build.json",
		"prepublish:npm": "npm run build",
		"publish:npm": "release-it",
		"prepublish:dev": "npm run build",
		"publish:dev": "npm publish --access public --tag dev",
		"prepare": "husky",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ignore-pattern .gitignore src/**/*.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:ci": "jest --ci --passWithNoTests --coverage"
	},
	"lint-staged": {
		"*.ts": "npm run format"
	},
	"directories": {
		"lib": "src",
		"test": "test"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"nestjs",
		"necord",
		"discord",
		"discord.js",
		"pagination"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/necordjs/pagination.git"
	},
	"bugs": {
		"url": "https://github.com/necordjs/pagination/issues"
	},
	"homepage": "https://github.com/necordjs/pagination#readme",
	"author": "Alexey Filippov <socket.someone@gmail.com>",
	"contributors": [
		"Alexey Filippov <socket.someone@gmail.com>"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"tslib": "^2.8.1"
	},
	"devDependencies": {
		"@commitlint/cli": "19.8.1",
		"@commitlint/config-angular": "19.8.1",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.26.0",
		"@favware/npm-deprecate": "2.0.0",
		"@nestjs/common": "11.1.6",
		"@nestjs/core": "11.1.6",
		"@nestjs/testing": "^11.1.0",
		"@release-it/conventional-changelog": "^10.0.1",
		"@types/jest": "^30.0.0",
		"@types/node": "24.3.0",
		"discord.js": "14.22.1",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "5.5.4",
		"globals": "^16.1.0",
		"husky": "9.1.7",
		"jest": "^30.0.0",
		"lint-staged": "16.1.6",
		"necord": "6.11.0",
		"prettier": "3.6.2",
		"reflect-metadata": "0.2.2",
		"release-it": "19.0.4",
		"rimraf": "6.0.1",
		"rxjs": "7.8.2",
		"ts-jest": "^29.3.2",
		"ts-node": "10.9.2",
		"typescript": "5.9.2",
		"typescript-eslint": "^8.32.0"
	},
	"peerDependencies": {
		"@nestjs/common": "^10.2.0 || ^11.0.0",
		"@nestjs/core": "^10.2.0 || ^11.0.0",
		"discord.js": "^14.0.1",
		"necord": "^6.0.0",
		"reflect-metadata": "^0.2.1",
		"rxjs": "^7.2.0"
	}
}
